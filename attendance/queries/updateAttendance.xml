<query id="updateAttendance" action="update">
    <tables>
        <table name="attendance" />
    </tables>
    <columns>
        <column name="regdate" var="regdate" notnull="notnull" />
        <column name="member_srl" var="member_srl" />
        <column name="ipaddress" var="ipaddress" />
    	<column name="greetings" var="greetings" />
	    <column name="today_point" var="today_point" />
    </columns>
    <conditions>
        <condition operation="equal" column="attendance_srl" var="attendance_srl"  />
    </conditions>
</query>
